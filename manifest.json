{
  "description": "Summarizes articles",
  "manifest_version": 3,
  "name": "TLDR",
  "version": "1.0",
  "icons": {
    "48": "icons/tldr-icon-48.png",
    "96": "icons/tldr-icon-96.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "extension_dist/popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'"
  }
}